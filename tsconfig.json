{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": "./"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "src/pages/*.tsx",
    "src/components/PageContainer/PortFolio.tsx",
    "src/components/PageContainer/FormList.tsx",
    "src/lib/miscrocms/client.ts",
    "src/pages/mainblog/[id].tsx",
    "src/components/PageContainer/BlogArchive.tsx",
    "src/pages/index.tsx",
    "src/components/Pagenation.tsx",
    "src/components/PageContainer/GitHub/index.ts",
    "src/components/PageContainer/GitHub/index.tsx",
    "src/components/Header/index.tsx",
    "src/components/Layout.tsx",
    "src/pages/todo-sample.jsx",
    "src/pages/usereducertest.tsx",
    "src/pages/_app.tsx"
, "src/pages/valtio.jsx", "src/components/reacthookform.tsx", "src/pages/redux-toolkit.tsx", "src/pages/recoil.jsx", "src/pages/add-recoil.jsx", "src/pages/syntaxtest.tsx", "src/pages/api/signup.tx"  ],
  "exclude": ["node_modules"]
}
